{
    "name": "neeto-api",
    "version": "1.0.0",
    "description": "A simple note taking API",
    "main": "src/app.ts",
    "scripts": {
        "start": "ts-node --files src/app.ts",
        "dev": "ts-node-dev --files --respawn src/app.ts",
        "build": "tsc",
        "prod": "node dist/app.js",
        "test": "jest --verbose --forceExit",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^8.8.4",
        "passport": "^0.5.0",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.26.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^18.11.18",
        "@types/supertest": "^6.0.2",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^10.1.2",
        "supertest": "^6.2.3",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "author": "",
    "license": "ISC"
}
